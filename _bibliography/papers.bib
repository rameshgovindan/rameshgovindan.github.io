  booktitle	= proc # "22nd" # ieee-icdcs,
  year		= 2002,
  publisher	= pub-ieee,
  address	= "Vienna, Austria",
  month		= {July},
  pages		= "457--458",
  note		= "See UCLA CSD TR-01-750 for an expanded version of this
